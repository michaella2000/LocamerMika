@model IEnumerable<LocamerMsySite.Models.Emplacement>

@{
    ViewBag.Title = "Index";
}
<h1>Application de Gestion Locamer</h1>
<h2>Index</h2>

<p>
    @Html.ActionLink("Créer un nouvel Emplacement", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.capacite)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.terrasse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Categorie.libelle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.capacite)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.terrasse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Categorie.libelle)
        </td>
        <td>
            @Html.ActionLink("Modifier", "Edit", new { id=item.code }) |
            
            @Html.ActionLink("Supprimer", "Delete", new { id=item.code })
        </td>
    </tr>
}

</table>
